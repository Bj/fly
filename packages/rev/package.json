  },
  "author": {
    "name": "Luke Edwards",
    "email": "luke@lukeed.com",
    "url": "https://lukeed.com"
  },
  "dependencies": {
    "rev-hash": "^1.0.0",
    "sort-keys": "^2.0.0"
  },
  "devDependencies": {
    "@taskr/clear": "*",
    "taskr": "*"
  },
  "engines": {
    "node": ">= 4.6"
  },
  "xo": {
